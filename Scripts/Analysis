# Análisis por tipo de mutación
Genie_all_samples <- read.delim("~/Desktop/MASTER/TFM/Datos R/GENIE/DATA COMPLETA alterations_across_samples (1).tsv")
View(Genie_all_samples)

# Columnas con alteraciones reales
alter_cols <- c("EGFR..MUT", "EGFR..FUSION", "EGFR..AMP", "EGFR..HOMDEL",
                "KRAS..MUT", "KRAS..FUSION", "KRAS..AMP", "KRAS..HOMDEL")

# Conteo de muestras por alteración
alter_counts <- sapply(alter_cols, function(col){
  sum(Genie_all_samples[[col]] != "no alteration", na.rm = TRUE)
})
alter_counts

# Función para calcular top 5 genes por tipo de alteración

top5_alterations <- function(df, col_name) {
  

  alterations <- df[[col_name]][df[[col_name]] != "no alteration"]   # Filtrar solo valores distintos de "no alteration"
  if(length(alterations) == 0) return(NULL)   # Si no hay alteraciones, devolver NULL
  
  counts <- table(alterations)   # Contar frecuencia de cada alteración
  top5 <- sort(counts, decreasing = TRUE)[1:min(5, length(counts))]   # Ordenar y tomar top 5
  top5_df <- data.frame(   # Convertir a data frame con porcentaje y grupo
    Group = col_name,
    Alteration = names(top5),
    Count = as.numeric(top5),
    Percent = round(100 * as.numeric(top5) / length(alterations), 2)
  )
  
  return(top5_df)
}

# Aplicar la función a todas las columnas de alteración
results_top5_list <- lapply(alter_cols, function(col) {
  top5_alterations(Genie_all_samples, col)
})

# Unir todos los resultados en un solo data.frame
results_top5_df <- do.call(rbind, results_top5_list)

# Ver resultados
results_top5_df

# Guardar 
write.table(results_top5_df, "results_top5_df_Todas las alteraciones de GENIE", sep="\t", row.names=FALSE)

