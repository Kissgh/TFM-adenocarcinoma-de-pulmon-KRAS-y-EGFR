# Análisis por tipo de mutación en px con mutación EFGR asociada

MUTACION.LIST.EGFR <- read.delim("~/Desktop/MASTER/TFM/Datos R/GENIE/MUTACION LIST EGFR.tsv")
View(MUTACION.LIST.EGFR)

mutation_counts <- table(MUTACION.LIST.EGFR$Mutation.Type)

mutation_df <- as.data.frame(mutation_counts)
colnames(mutation_df) <- c("Mutation.Type", "Count")

mutation_df$Percent <- round(100 * mutation_df$Count / sum(mutation_df$Count), 2) # Calcular porcentaje

mutation_df <- mutation_df[order(-mutation_df$Count), ] # Ordenar de mayor a menor

mutation_df

# Guardar 
write.table(mutation_df, "mutation_df_EGFR", sep="\t", row.names=FALSE)


